<div>

  <table data-toggle="table">
    <thead>
      <tr>
        <th data-field="id">Id</th>
        <th data-field="stocks">Stocks</th>
        <th data-field="worth">Net Worth</th>
        <th data-field="bid">Bid</th>
        <th data-field="ask">Ask</th>
      </tr>
    </thead>
    <tbody>
      

      <% marketData.each do |md| %>
            <tr>
              <td><%= md.getId() %></td>
              <td><%= md.getStock() %></td>
              <td><%= md.getWorth() %></td>
              <td><%= md.getBid() %></td>
              <td><%= md.getAsk() %></td>
            </tr>
          <% end %>
        </tbody>

  </table>
</div>
